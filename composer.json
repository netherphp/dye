{
	"name": "netherphp/dye",
	"description": "Actual simple colour manipulation.",
	"keywords": [ "php", "colour" ],
	"license": "BSD-2-Clause",
	"authors":[
		{
			"name": "Bob Majdak Jr",
			"email": "bob@pegasusgate.net",
			"homepage": "http://pegasusgate.net/+bob",
			"role": "developer"
		}
	],
	"config": {
		"preferred-install": {
			"netherphp/*": "source",
			"*": "dist"
		},
		"allow-plugins": {
			"netherphp/composer-plugin": true,
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"require": {
		"php": "^8.1"
	},
	"require-dev": {
		"netherphp/standards": ">=0.2.10",
		"dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
		"phpunit/phpunit": "^11.2"
	},
	"bin": [
	],
	"autoload": {
		"psr-0": {
			"Nether\\":"src/"
		}
	}
}
